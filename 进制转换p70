#include<stdio.h>//15 Aab3 7 既将Aab3 从 15进制转成7进制  方法用十进制作为过渡  具体见王道进制转换章  
#include<string.h>
int main()
{
	int a,b; 
	char ans[10000];
	while(scanf("%d%s%d",&a,ans,&b)!=EOF)
	{
		int i,len=strlen(ans);
		 int temp=0,c=1;
		for(i=len-1;i>=0;i--)//Aab b处在低位 但是在字符数组里 他排在高位 所以要从位数大的数
		{
			int x; 
			if(ans[i]>='0'&&ans[i]<='9')
			{
				x=ans[i]-48;
		
			 } 
			 else if(ans[i]>='a'&&ans[i]<='z')
			 {
			 x=ans[i]-'a'+10;	
			  } 
			  else 
			  {
			  	x=ans[i]-'A'+10;
			  }	
			 
			  		temp=temp+x*c;
			  		c*=a;
			  		  
		}
		char str[1000],size=0 ;
		
		do
		{
	 int m=temp%b;
	str[size++]=(m<10)?m+48:m-10+'A';
	temp/=b;
		}while(temp);
		for(i=size-1;i>=0;i--)
		{
		printf("%c",str[i]);	
		}
printf("\n");		
	}
	
	
	
	
	
	return 0;
 } 
